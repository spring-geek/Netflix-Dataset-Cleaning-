  {
   "cell_type": "code",
   "execution_count": 310,
   "id": "5a62e3a4-abcb-4f7f-9414-41b0eaa9b3ae",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Changing the way the dates are viewed so that it can be read easily\n",
    "df['Date_added'] = pd.to_datetime(df['Date_added'].str.strip())\n",
    "df['Date_added'] = df['Date_added'].dt.strftime('%d.%m.%y')"
   ]
